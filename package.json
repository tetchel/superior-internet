{
  "name": "superior-internet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./bin/www",
    "server-dev": "nodemon --watch routes/ --watch public/ ./bin/www",
    "client": "parcel watch client/*.js --out-dir dist/",
    "start": "concurrently --kill-others \"npm run client\" \"npm run server-dev\""
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "gfycat-style-urls": "^1.0.3",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "json-hash": "^1.2.0",
    "mongodb": "^3.1.6",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.4",
    "p5": "^0.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-p5-wrapper": "0.0.4",
    "socket.io": "^2.1.1",
    "trianglify": "^1.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^4.0.1",
    "parcel": "^1.9.7"
  }
}
